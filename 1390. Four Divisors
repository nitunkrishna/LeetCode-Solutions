class Solution
{
public:
    int sumFourDivisors(vector<int>& nums)
    {
        int res=0;
        for(int i=0; i<nums.size(); i++)
        {
            int div=0, sum=0;
            for(int j=1; j*j<=nums[i]; j++)
            {
                if(nums[i]%j==0)
                {
                    div++;
                    sum+=j;
                    if (j!=nums[i]/j)
                    {
                        div++;
                        sum+=nums[i]/j;
                    }
                }
            }
            if(div==4) res+=sum;
        }
        return res;
    }
};
