class Solution
{
public:
    int countTriples(int n)
    {
        int a, b,c, x, cnt=0;
        for(int a=1; a<=n; a++)
        {
            for(int b=1; b<=n; b++)
            {
                x=(a*a)+(b*b);
                c=sqrt(x);
                if((c*c)==x && c<=n) cnt++;
            }
        }
        return cnt;
    }
};
